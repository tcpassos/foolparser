// Declaração de uma classe com atributos e métodos
class MinhaClasse {
    // Atributos
    int numero;
    bool ativo;

    // Método sem argumentos
    void iniciar() {
        if (ativo) {
            numero = 10;
        } else {
            numero = 0;
        }
    }

    // Método com argumentos
    int somar(int a, int b) {
        return a + b;
    }
}
