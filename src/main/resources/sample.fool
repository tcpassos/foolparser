{ class SampleClass
    int num;
    int num2;
    bool flag;

    int sum(int a, int b) {
        return a + b;
    }

    int test() {
        num = 1;
        num2 = 2;
        return sum(num, num2);
    }

    void main() {
        flag = not False;
        if (flag) num = 10; else num = 0;
        while (num < 20) num = num + 1;
        test();
    }
}